// калькулятор

Количество рулонов у тебя - 1 = a
Посещений туалета в день - 2 = b

Среднее количество бумажек за одно посещение - 2 = c
Бумажка состоит из количества салфеток - 2 = d
Салфеток в одном рулоне - 140 = e
Людей в семье - 1 = f
Дней карантина - 20 = g


всего салфеток вообще = рулоны (a)  * салфеток в рулоне (e)
один рулон (в бумажках) = рулон (e) / количество бумажек (c) / салфеток в бумажке (d)
расход за один поход (в салфетках) =  салфетки (d) * бумажки (c)
расход за день на 1 человека (в салфетках) = салфетки (d) * бумажки (c) * посещений (b)
расход за день всего (в салфетках) = салфетки (d) * бумажки (c) * посещений (b) * людей (f)
расход за карантин (в салфетках) = салфетки (d) * бумажки (c) * посещений (b) * людей (f) * дней карантина (g)

расход за карантин (сколько нужно бумаги) (в рулонах) = (салфетки (d) * бумажки (c) * посещений (b) * людей (f) * дней карантина (g)) / салфеток в рулоне (e)
всего хватит рулонов = (рулоны (a)  * салфеток в рулоне (e)) / (салфетки (d) * бумажки (c) * посещений (b) * людей (f))

const result = (a, b, c, d, e, f) => {
    const total = (a * e) / (d * c * b * f)
}

процент = результат / дней карантина * 100

const percent = (result, g) => {
    const total = result / g * 100;
}


